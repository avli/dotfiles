if [[ -n $TERMINAL_EMULATOR && $TERMINAL_EMULATOR = "JetBrains-JediTerm" ]]; then
  alias ls="exa --git --icons"
else
  alias ls="exa --git"
fi
alias la="ls -a"
alias ll="ls -l"
alias lla="ll -a --grid"
alias tree="ls -T"
alias :q="exit"
alias :q!="exit"
alias :e="vim"
alias vi=vim
alias emacs="emacs -nw"
alias e="emacs -nw"
alias st="git status"
alias co="git commit -m"
alias a="git add"
alias idea="idea -e"
